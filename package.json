{
    "name": "ldif",
    "displayName": "LDIF syntax",
    "description": "VS Code extension for LDIF syntax highlighting",
    "version": "0.1.0",
    "publisher": "jtavin",
    "repository": "https://github.com/J00MZ/vscode-ldif-ext",
    "icon": "https://github.com/J00MZ/vscode-ldif-ext/images/ldif.png",
    "engines": {
        "vscode": "^1.19.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "ldif",
            "aliases": ["LDIF", "ldif"],
            "extensions": [".ldif",".ldf",".LDIF",".LDF"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "ldif",
            "scopeName": "text.ldif",
            "path": "./syntaxes/ldif.tmLanguage"
        }]
    }
}